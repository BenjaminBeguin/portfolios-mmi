<h1>All user</h1>


<div>
	<% if user_signed_in? %>

	  	<%= button_to "sign out", destroy_user_session_path, method: :delete %>
	<% end %>
</div>

<% @users.each do |user| %>
	<div>
		<li><%= user.id %></li>
		<li><%= user.firstname %></li>
		<li><%= user.lastname %></li>
		<li><%= user.email %></li>
		<li><%= user.job.label if user.job  %></li>
		<li><%= user.visite %> visite</li>
		<li></li>
		<li><%= user.portfolio.url if user.portfolio %></li>
		<li><%= user.portfolio.like if user.portfolio %> like</li>
		<li><%= user.portfolio.visite if user.portfolio %> visite</li>
	</div>

	<br><br>
	

<% end %>

